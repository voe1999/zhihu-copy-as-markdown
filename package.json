{
  "name": "zhihu-downloader",
  "version": "0.3.65",
  "scripts": {
    "dev": "run-s build:test start",
    "start": "run-p watch serve",
    "serve": "live-server ./dist",
    "watch": "webpack --mode development --watch",
    "build:ts": "tsc",
    "build:vite": "vite build",
    "build:production": "webpack --mode production",
    "build:tampermonkey": "node src/scripts/build-tampermonkey.js",
    "build": "run-s build:ts build:vite build:update build:tampermonkey",
    "build:update": "node src/scripts/add-version.js",
    "build:test": "node src/scripts/build-test-html.js",
    "lint": "eslint --fix --ext .js,.ts ./src ./scripts webpack.config.js"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/jszip": "^3.4.0",
    "@types/md5": "^2.3.4",
    "@types/node": "^20.7.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "live-server": "^1.2.2",
    "md5": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "shadow-cljs": "^3.1.7"
  },
  "type": "module",
  "author": "HowardZhangdqs",
  "license": "MIT"
}