{
  "name": "zhihu-downloader",
  "version": "0.3.71",
  "scripts": {
    "dev": "run-s build:test start",
    "start": "run-p watch serve",
    "serve": "live-server ./dist",
    "watch": "webpack --mode development --watch",
    "build:ts": "tsc",
    "build:cljs": "shadow-cljs release zhihu-downloader",
    "build:vite": "vite build",
    "build:production": "webpack --mode production",
    "build:tampermonkey": "node scripts/build-tampermonkey.js",
    "build": "run-s build:ts build:cljs build:vite build:tampermonkey",
    "build:update": "node scripts/add-version.js",
    "build:test": "node scripts/build-test-html.js",
    "publish": "run-s build:ts build:cljs build:vite build:update build:tampermonkey",
    "lint": "eslint --fix --ext .js,.ts ./src ./scripts webpack.config.js",
    "test": "jest --runInBand --passWithNoTests"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^30.0.0",
    "@types/jszip": "^3.4.0",
    "@types/md5": "^2.3.4",
    "@types/node": "^20.7.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-jest": "^30.0.4",
    "eslint": "^8.52.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "jsdom": "^26.1.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "vite": "^5.2.0"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "live-server": "^1.2.2",
    "md5": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "shadow-cljs": "^3.1.7",
    "ts-node": "^10.9.2"
  },
  "type": "module",
  "author": "HowardZhangdqs",
  "license": "MIT"
}
